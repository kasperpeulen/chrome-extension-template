{
  "manifest_version": 2,
  "background": {
    "persistent": true,
    "scripts": ["background/index.js"]
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["index.js"]
    }
  ],
  "permissions": [         "tabs",
    "notifications",
    "http://*/",
    "https://*/" ]
}
